[build]
  functions = "functions"

[[redirects]]
  from = "/api/v1/app/*"
  to = "/.netlify/functions/app"
  status = 200

[[redirects]]
  from = "/api/v1/auth/*"
  to = "/.netlify/functions/auth"
  status = 200

[[redirects]]
  from = "/api/v1/file/*"
  to = "/.netlify/functions/file"
  status = 200

[[redirects]]
  from = "/api/v1/language/*"
  to = "/.netlify/functions/language"
  status = 200

[[redirects]]
  from = "/api/v1/mailer/*"
  to = "/.netlify/functions/mailer"
  status = 200

[[redirects]]
  from = "/api/v1/project/*"
  to = "/.netlify/functions/project"
  status = 200

[[redirects]]
  from = "/api/v1/user/*"
  to = "/.netlify/functions/user"
  status = 200

[[redirects]]
  from = "/*"
  to = "/index.html"
  status = 200